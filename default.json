{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":semanticCommits"
  ],
  "configMigration": true,
  "dependencyDashboardOSVVulnerabilitySummary": "all",
  "hostRules": [
    {
      "matchHost": "api.github.com",
      "hostType": "github",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "osvVulnerabilityAlerts": true,
  "schedule": [
    "at any time"
  ],
  "timezone": "America/New_York"
}
